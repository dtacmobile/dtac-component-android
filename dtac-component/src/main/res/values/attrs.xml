<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseComponent">
        <attr name="isScale" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DtacMultiBannerView">
        <attr name="interval" format="integer" />
        <attr name="isAutoScroll" format="boolean" />
        <attr name="isReverse" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DtacTextField">
        <attr name="regexType" format="enum">
            <enum name="msisdn" value="1" />
            <enum name="email" value="2" />
        </attr>
    </declare-styleable>
</resources>